{
  "name": "base58-monero",
  "version": "0.0.1",
  "description": "Base58 Monero style",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoinSpace/base58-monero.git"
  },
  "keywords": [
    "base58",
    "monero",
    "crypto"
  ],
  "license": "MIT",
  "dependencies": {
    "bn.js": "^4.12.0"
  },
  "devDependencies": {
    "@coinspace/eslint-config": "^1.0.2",
    "eslint": "^7.20.0",
    "jest": "^26.6.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": [
      "@coinspace"
    ],
    "env": {
      "es2020": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ]
    },
    "overrides": [
      {
        "files": ["*.test.js"],
        "env": {
          "jest": true
        }
      }
    ]
  }
}

{
  "name": "base58-monero",
  "version": "0.0.4",
  "description": "Base58 Monero style",
  "author": "Coin Crypto Wallet <support@coin.space> (https://coin.space/)",
  "main": "./index.cjs",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "type": "module",
  "scripts": {
    "build": "babel index.js --out-file-extension=.cjs --out-dir=./",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoinSpace/base58-monero.git"
  },
  "keywords": [
    "base58",
    "monero",
    "crypto"
  ],
  "license": "MIT",
  "dependencies": {
    "bn.js": "^4.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "eslint": "^7.20.0",
    "eslint-config-coinspace": "^1.1.1",
    "jest": "^26.6.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "12"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": [
      "coinspace"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  }
}
